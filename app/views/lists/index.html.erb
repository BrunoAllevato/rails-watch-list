<div class="container mt-3">

  <h1>My Movie Lists</h1>

  <ul>
    <% @lists.each do |list| %>
      <%= link_to list.name, list_path(list) %>
    <% end %>
  </ul>

  <%= link_to new_list_path, class: "text-decoration-none" do %>
    <i class="fa-solid fa-plus"></i>
    Create List
  <% end %>
</div>
